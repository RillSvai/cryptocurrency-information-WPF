<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="borderGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#6D2FFF" Offset="0"/>
        <GradientStop Color="#FB539B" Offset="0.5"/>
        <GradientStop Color="#836EFB" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="primaryBackGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#0A003A" Offset="0"/>
        <GradientStop Color="#1C0E49" Offset="0.50"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="secondaryBackGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#24105F" Offset="0"/>
        <GradientStop Color="#210F55" Offset="0.50"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="panelOverColor" Color="#281269"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#2B1372"/>

</ResourceDictionary>