<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="borderGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#411C00" Offset="0"/>
        <GradientStop Color="#FF663C" Offset="0.5"/>
        <GradientStop Color="#FFC047" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="primaryBackGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#1E0000" Offset="0"/>
        <GradientStop Color="#230000" Offset="0.50"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="secondaryBackGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#2C0000" Offset="0"/>
        <GradientStop Color="#330000" Offset="0.50"/>
    </LinearGradientBrush>
</ResourceDictionary>